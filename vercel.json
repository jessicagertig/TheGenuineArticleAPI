{
  "version": 2,
  "name": "the-genuine-article-api",
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ],
  "crons": [
    {
      "schedule": "55 17 * * *",
      "path": "/api/jobs.js"
    }
  ]
}
